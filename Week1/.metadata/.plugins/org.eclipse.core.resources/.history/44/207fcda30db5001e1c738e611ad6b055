package pojo;

public class Employee {
	
	private int eid;
	private String ename;
	private double salary;
	
	public Employee() {
		super();
	}
	
	public Employee(int eid, String name, double salary ){
		this();
		this.eid = eid;
		this.ename = name;
		this.salary = salary;
	}
	
	public void setEid(int eid) {
		this.eid = eid;
	}

}
